{% import 'components/atoms/card.njk' as card %}

<ul class="list">
  {%- for weeknote in collections.weeknote | reverse -%}
    {{ card.list
      (
        weeknote.url,
        weeknote.data.title,
        weeknote.templateContent | safe,
        weeknote.data.date,
        weeknote.data.type
      )
    }}
  {%- endfor -%}
</ul>
